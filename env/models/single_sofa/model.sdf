<?xml version='1.0'?>
<sdf version="1.5">

  <model name="single_sofa">
    <static>false</static>
    <link name="link">

      <inertial>
        <mass>10</mass>
        <inertia>
          <ixx>1.0</ixx>       <!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
          <ixy>0.0</ixy>       <!-- for a box: ixy = 0 -->
          <ixz>0.0</ixz>       <!-- for a box: ixz = 0 -->
          <iyy>1.0</iyy>       <!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
          <iyz>0.0</iyz>       <!-- for a box: iyz = 0 -->
          <izz>1.0</izz>      <!-- for a box: izz = 0.083 * mass * (x*x + y*y) -->
        </inertia>
      </inertial>

      <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://single_sofa/single_sofa.dae</uri>
          </mesh>
        </geometry>
      </collision>

      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://single_sofa/single_sofa.dae</uri>
          </mesh>
        </geometry>
      </visual>

    </link>
  </model>

</sdf>
